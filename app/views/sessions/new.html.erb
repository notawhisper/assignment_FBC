<p id="notice"><%= notice %></p>
<h1 class="page_title">Log in</h1>

<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= f.label :email , class:"labels" %>
  <%= f.email_field :email %>

  <%= f.label :password, class:"labels"  %>
  <%= f.password_field :password %>
  <br><br>
  <%= f.submit "Log in" %>
<% end %>
